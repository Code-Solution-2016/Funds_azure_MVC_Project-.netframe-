@page
@model homeModel
@{
}

@inject IHttpContextAccessor HttpContextAccessor
<!-- Include the menu here -->
@Html.Partial("_Menu")

<!-- Styles -->
<style>

    .hero {
        background-image: url('https://th.bing.com/th/id/R.4f19af98b396247a530bfbca1f66d036?rik=hTyS8TaAprJt7g&pid=ImgRaw&r=0');
        height: 500px;
        display: flex;
        justify-content: center;
        align-items: center;
        color: white;
        text-align: center;
        background-size: cover;
        position: relative;
    }

        .hero::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            background-color: rgba(0, 0, 0, 0.7);
          
        }

    .hero-content {
        position: relative;
        
    }

        .hero-content h1 {
            font-size: 3em;
        }

    .mission, .impact, .get-involved {
        padding: 50px 0;
        text-align: center;
    }

    .impact-content {
        display: flex;
        justify-content: space-around;
    }

    .stat {
        text-align: center;
    }

    .btn {
        padding: 10px 20px;
        margin: 10px;
        border: none;
        border-radius: 5px;
        background-color: grey;
        color: white;
        cursor: pointer;
    }

    footer {
        background-color: #333;
        color: white;
        padding: 20px;
        text-align: center;
    }

    .social-links a {
        color: white;
        margin: 0 10px;
        text-decoration: none;
    }

        .social-links a:hover {
            text-decoration: underline;
        }
</style>

<!-- Hero Section with Background Image -->
<br />
<div class="hero">
    <div class="hero-content">
        <h1>Join the Movement for Change</h1>
        <br />
        <p style="width:80%;margin-left:10%;margin-right:10%"> At the Gift of the Givers Foundation, we believe that every helping hand counts. Volunteers play a crucial role in our mission to assist those in needt</p>

       
    </div>
</div>

<!-- Mission Statement Section -->
<section class="mission">
    <div class="container">
        <h2>Our Mission</h2><br /><br />
        <p style="width:80%;margin-left:10%;margin-right:10%">At Gift of the Givers Foundation<br /><br /> our mission is to alleviate suffering and empower communities in need. We are dedicated to providing essential resources such as food, medical aid, and education, ensuring that every individual has access to the support they require.
            We strive to foster hope for a better future by not only addressing immediate needs but also creating sustainable solutions that promote resilience and self-sufficiency. Together, we aim to build a more compassionate world, where every person has the opportunity to thrive.</p>
    </div>
</section>


<!-- Impact Section with Photos and Statistics -->
<section class="impact">
    <div class="container">
        <h2>Our Impact</h2><br /><br />
        <div class="impact-content" style="display:flex;flex-wrap:wrap">
            <div class="stat">
                <img style="border-radius:5rem" src="data:image/jpeg;base64," alt="Relief Image" />
                <h3>500,000+</h3>
                <p>Meals Provided</p>
                <br />
               
            </div>
            <div class="stat">
                <img  style="border-radius:5rem" src="data:image/jpeg;base64," alt="Water Relief Image" />
                <h3>300+</h3>
                <p>Water Projects</p>
                <br />
               
            </div>
            <div class="stat">
                <img style="border-radius:5rem" src="https://th.bing.com/th/id/OIP.U8s_jIObsgqSGYFa19apRwAAAA?w=248&h=180&c=7&r=0&o=5&dpr=1.3&pid=1.7" alt="Disaster Relief Image" />
                <h3>100+</h3>
                <p>Disaster Relief Operations</p>
                <br />
                
            </div>
        </div>
    </div>
</section>

<!-- Get Involved Section -->
<section class="get-involved">
    <div class="container">
        <h2>How You Can Help</h2><br /><br />
<p style="width:80%; margin-left:10%; margin-right:10%;">
    We rely on the generosity of individuals like you to continue our vital work in the community. Your contributions, whether through financial donations, volunteering your time, or spreading awareness, play a crucial role in our efforts. Every act of kindness helps us provide food, medical supplies, and educational resources to those in need. Together, we can create lasting change and uplift the lives of countless individuals and families, ensuring that hope and support are always within reach.
</p>
   
    </div>
</section>






@Html.Partial("_footer")
